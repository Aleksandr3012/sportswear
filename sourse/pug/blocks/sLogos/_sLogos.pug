mixin sLogos()
	// start sLogos
	+b.sLogos#sLogos&attributes(attributes)
		.container
			.section-title-vertical
				h2  Brands
			+e.slider--js.swiper-container
				.swiper-wrapper
					- for (var x = 1; x < 6; x++)
						+e.slide.swiper-slide: +e.img-wrap
							img.swiper-lazy(src="#" data-src=`img/@2x/logo-${x}.png`, alt="")
					- for (var x = 1; x < 6; x++)
						+e.slide.swiper-slide: +e.img-wrap
							img.swiper-lazy(src="#" data-src=`img/@2x/logo-${x}.png`, alt="")
	// end sLogos